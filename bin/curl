create_table() {
  curl -v -X POST \
     -H "Content-Type: application/json" \
     -d @bin/table.json \
     http://ruby-on-rails-116071.nitrousapp.com:3000/tables
}

create_order() {
  curl -v -X POST \
      -H "Content-Type: application/json" \
      -d @bin/order.json \
     http://ruby-on-rails-116071.nitrousapp.com:3000/tables/1/orders
}

$1